{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "no-param-reassign": [0],
    "no-use-before-define": ["error", { "functions": false }]
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "settings": {
    "html/html-extensions": [
      ".html"
    ]
  }
}
