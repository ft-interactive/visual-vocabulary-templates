{
  "name": "visual-vocabulary-templates",
  "version": "1.0.0",
  "description": "A library of data driven graphics -- to be used as starting points for static graphics to be further refined through Illustrator. [Viewable here](http://ft-interactive.github.io/visual-vocabulary/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ft-interactive/visual-vocabulary-templates.git"
  },
  "keywords": [],
  "author": "Financial Times Interactive Graphics",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ft-interactive/visual-vocabulary-templates/issues"
  },
  "homepage": "https://github.com/ft-interactive/visual-vocabulary-templates#readme",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.6.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "no-param-reassign": [
        0
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "max-len": ["error", {
        "ignoreTrailingComments": true
      }]
    },
    "env": {
      "browser": true
    },
    "plugins": [
      "html"
    ],
    "settings": {
      "html/html-extensions": [
        ".html"
      ]
    },
    "globals": {
      "SystemJS": false
    }
  },
  "dependencies": {
    "d3": "^4.9.1",
    "g-chartcolour": "^0.8.11",
    "locate-path": "^2.0.0"
  }
}
